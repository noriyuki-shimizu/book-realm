    "ress": "^5.0.2",
    "snakecase-keys": "^8.0.1",
    "vite-svg-loader": "^5.1.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.14.3",
    "@vitest/coverage-v8": "^2.1.2",
    "@vue/test-utils": "^2.4.6",
    "dprint": "^0.47.2",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^15.7.4",
